{
  "name": "@techbasejs/simm-firebase-chat",
  "version": "1.0.0",
  "description": "Missing ECMAScript module utils for Node.js",
  "repository": "techbasejs/techbasejs",
  "license": "MIT",
  "sideEffects": false,
  "type": "commonjs",
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    }
  },
  "dependencies": {
    "lit": "3.2.1",
    "@lit/context": "1.1.3",
    "firebase": "10.14.0",
    "tailwindcss": "3.4.13"
  },
  "devDependencies": {
    "@types/node": "20.14.9"
  }
}
