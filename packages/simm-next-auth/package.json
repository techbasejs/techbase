{
  "name": "@techbase/simm-next-auth",
  "version": "1.0.1",
  "description": "Missing ECMAScript module utils for Node.js",
  "repository": "techbase/techbasejs",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "play": "next dev playground",
    "lint": "eslint src test && prettier -c src test",
    "lint:fix": "eslint src test --fix && prettier -w src test",
    "release": "pnpm build && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "cookie": "^0.6.0",
    "jose": "^5.3.0",
    "next-simple-auth": "link:",
    "ofetch": "^1.3.4"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "changelogen": "^0.5.5",
    "eslint": "^9.2.0",
    "eslint-config-unjs": "0.3.1",
    "jiti": "^1.21.0",
    "next": "^14.2.3",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@9.1.0",
  "peerDependencies": {
    "next": "^14.2.3",
    "react": "^18.3.1"
  }
}